{
  "buckets": [
    "main",
    "sboot@https://github.com/xfournet/scoot-sboot"
  ],
  "apps": [
    "######## Core Scoop dependencies ########",
    "aria2",
    "7zip",
    "git",
    "sboot/sboot",
    ""
  ],
  "exts": [
    {
      "app": "7zip",
      "parameters": {
        "excludedExtensions": [
          "iso",
          "vhd"
        ]
      }
    },
    {
      "app": "git",
      "parameters": {
        "overwriteUserGitconfig": false
      }
    },
    {
      "app": "hxd"
    },
    {
      "app": "keepass"
    },
    {
      "app": "notepadplusplus"
    },
    {
      "app": "vim",
      "parameters": {
        "overwriteUserVimrc": false
      }
    },
    {
      "app": "winmerge"
    }
  ]
}
